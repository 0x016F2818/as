<?xml version='1.0' encoding='utf-8'?>
<AS>
	<Os>		
		<Task name='TaskApp' priority='5' exe-priority='5' max-activation='1'
              auto-start='true'   
		      app-mode='OSDEFAULTAPPMODE' stack-size='512' >
		    <!-- put auot mask event firstly and then those non auto mask event -->
		    <!-- note: the auto mask start from 0x00000001,0x00000002, (1<<index) ... -->
    		<Event name='Event1' mask='auto'/>
    		<Event name='Event2' mask='auto'/>
		</Task>
		<Alarm name='Alarm10ms' counter='OsClock' auto-start='true' app-mode='OSDEFAULTAPPMODE' offset='5' period='10' />
		<Alarm name='Alarm20ms' counter='OsClock' auto-start='true' app-mode='OSDEFAULTAPPMODE' offset='10' period='20' />
		<Alarm name='Alarm50ms' counter='OsClock' auto-start='true' app-mode='OSDEFAULTAPPMODE' offset='15' period='50' />
		<Alarm name='Alarm100ms' counter='OsClock' auto-start='true' app-mode='OSDEFAULTAPPMODE' offset='20' period='100' />	
 	</Os>
</AS>